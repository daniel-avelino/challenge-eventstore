package net.intelie.challenges;

import java.util.ArrayList;
import java.util.List;

public class EventIteratorService implements EventIterator {

	List<Event> eventsIterator = new ArrayList<>();

	public EventIteratorService(List<Event> eventsIterator) {
		this.eventsIterator = eventsIterator;
	}

	@Override
	public void close() throws Exception {

	}

	@Override
	public boolean moveNext() {
		return this.eventsIterator.listIterator().hasNext();
	}

	@Override
	public Event current() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void remove() {
		// TODO Auto-generated method stub

	}

}
